---
---
<!doctype html>
<html>
	<head>
		{% include head.liquid %}
		<link rel="stylesheet" media="screen" type="text/css" href="{{ '/style.css' | relative_url }}">
		<link rel="icon" type="image/png" href="favicon.png">
		<link rel="canonical" href="https://games.tigeroakes.com/minecraft-tools{{ page.url }}">
	</head>
	<body>
		<header class="page-header">
			<nav class="navigation-container">
				<div class="center navigation-tabs">
					<h1 class="site-title">{{ site.title }}</h1>
					<ul class="navigation invisible">
						{% for nav in site.data.navigation %}
							<li class="nav-link-container">
							{% if nav.link == page.url %}
								<a
									class="invisible nav-link nav-link-here"
									href="{{ nav.link | relative_url }}"
								>
									{{ nav.title }}
								</a>
							{% else %}
								<a
									class="invisible nav-link"
									href="{{ nav.link | relative_url }}"
								>
									{{ nav.title }}
								</a>
							{% endif %}
							</li>
						{% endfor %}
					</ul>
					<a href="https://games.tigeroakes.com" class="about invisible nav-link">
						Created by <img src="{{ '/tigeroakes.svg' | relative_url }}" alt="Tiger Oakes" width="160" height="32" />
					</a>
				</div>
			</nav>
			<div class="center">
				<h1 class="page-title">{{ page.title }}</h1>
			</div>
		</header>
		<main class="center">
			{{ content }}
		</main>

		{% if page.script %}
			<script defer src="script.js"></script>
		{% endif %}
	</body>
</html>
